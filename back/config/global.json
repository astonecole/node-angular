{
    "app": {
        "name": "Aston API Global",
        "version": "0.1.0"
    },
    "db": {
        "default": {
            "url": "mysql://dev:dev@localhost:3310/jobapi"
        }
    },
    "security": {
        "pem": {
            "days": 30,
